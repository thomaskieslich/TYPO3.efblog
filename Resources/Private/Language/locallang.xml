<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>Language labels for the Simple Blog extension in the FRONTEND</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="post">Post</label>
			<label index="post_title">title</label>
			<label index="post_category">post categories</label>
			<label index="post_comments">post comments</label>
			<label index="category">Category</label>
			<label index="comment">Comment</label>
			
			<!--Links-->
			<label index="link_backTo">Back to: </label>
			
			<!--Paginator-->
			<label index="paginator_first">&lt;&lt; first</label>
			<label index="paginator_previous">&lt; previous</label>
			<label index="paginator_next">next &gt;</label>
			<label index="paginator_last">last &gt;&gt;</label>
			
			<!--List-->
			<label index="teaser_more"><![CDATA[ …]]></label>
			
			<!--Latest Widget-->
			<label index="widget_latest_more"><![CDATA[ …]]></label>
			
			<!--CategoryWidget-->
			<label index="widget_category_more"><![CDATA[ …]]></label>
			
			<!--Error Messages-->
			<label index="notice_error">Error!</label>
			<label index="notice_noPost">There is no Post to show!</label>
		</languageKey>
	</data>
</T3locallang>